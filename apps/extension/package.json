{
  "name": "locatorjs-extension",
  "version": "1.2.7",
  "description": "LocatorJS Chrome Extension - option-click to code (ReactJS)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/infi-pc/locatorjs-extension.git"
  },
  "scripts": {
    "build": "node utils/build.js",
    "build:firefox": "TARGET_BROWSER=firefox node utils/build.js",
    "lint": "eslint src --ext .ts,.tsx",
    "dev": "node utils/webserver.js",
    "dev:firefox": "TARGET_BROWSER=firefox node utils/webserver.js",
    "start:firefox": "web-ext run --source-dir ./build/production_firefox",
    "pack:firefox": "web-ext build --source-dir ./build/production_firefox --artifacts-dir ./build/artifacts_firefox",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "dependencies": {
    "@hope-ui/solid": "^0.6.1",
    "@locator/shared": "*",
    "@stitches/core": "^1.2.8",
    "@tailwindcss/forms": "^0.5.1",
    "@types/semver": "^7.3.9",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.5.0",
    "posthog-js": "^1.23.0",
    "raw-loader": "^4.0.2",
    "semver": "^7.3.7",
    "solid-icons": "^0.4.12",
    "solid-js": "^1.6.9",
    "solid-transition-group": "^0.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.16.7",
    "@types/chrome": "^0.0.161",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-solid": "^1.3.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^6.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-flowtype": "^6.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "html-loader": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "sass-loader": "^12.2.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.24",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "web-ext": "^7.1.1",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  }
}
